{
  "name": "pigsa_api",
  "version": "1.0.0",
  "description": "API PIGSA (Express + Sequelize + MariaDB + TS + Parcel)",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --watch src --ext ts,js,json --exec ts-node src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js"
  },
  "targets": {
    "main": {
      "source": "src/index.ts",
      "distDir": ".",
      "includeNodeModules": true,
      "context": "node"
    }
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "formidable": "^2.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.3",
    "mariadb": "^3.4.5",
    "morgan": "^1.10.0",
    "mysql2": "^3.16.2",
    "pg": "^8.13.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/formidable": "^3.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.7",
    "@types/node": "^22.7.4",
    "nodemon": "^3.1.11",
    "parcel": "^2.16.4",
    "sequelize-auto": "^0.8.8",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
